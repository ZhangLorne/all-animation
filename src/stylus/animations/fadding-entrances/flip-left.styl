.flip-left {
	setAnimation(flipLeft 1 1s ease-out forwards);
}


/* FLIP LEFT */
@-webkit-keyframes flipLeft {
	0%{
		-webkit-transform: translateX(-100px);
		opacity: 0;
	}
	70%{
		-webkit-transform: translateX(0);
		opacity: 1;
	}
	100% {
		opacity: 1;
		visibility: visible;
	}
}
